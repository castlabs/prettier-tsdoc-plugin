{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": ["./node_modules/typedoc/tsdoc.json"],
  "supportForTags": {
    "@alpha": true,
    "@beta": true,
    "@decorator": true,
    "@deprecated": true,
    "@defaultValue": true,
    "@eventProperty": true,
    "@example": true,
    "@experimental": true,
    "@inheritDoc": true,
    "@internal": true,
    "@label": true,
    "@link": true,
    "@override": true,
    "@packageDocumentation": true,
    "@param": true,
    "@privateRemarks": true,
    "@public": true,
    "@readonly": true,
    "@remarks": true,
    "@returns": true,
    "@sealed": true,
    "@see": true,
    "@throws": true,
    "@typeParam": true,
    "@virtual": true,

    "@category": true,
    "@categoryDescription": true,
    "@document": true,
    "@enum": true,
    "@event": true,
    "@group": true,
    "@groupDescription": true,
    "@hidden": true,
