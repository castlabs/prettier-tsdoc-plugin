{
  "name": "@castlabs/prettier-tsdoc-plugin",
  "version": "0.5.1",
  "description": "A prettier plugin to handle TSDoc comments",
  "license": "MIT",
  "author": "castLabs",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --log-level warn --write .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "check": "npm run build && npm run prettier && npm run typecheck && npm run lint:fix && npm run test",
    "example:prettier": "cd example && npm run prettier",
    "example:typedoc": "cd example && npm run typedoc"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-tsdoc": "^0.4.0",
    "rollup": "^4.46.1",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  }
}
