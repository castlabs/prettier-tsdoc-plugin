{
  "name": "prettier-tsdoc-plugin",
  "version": "0.5.0",
  "description": "A prettier plugin to handle TSDoc comments",
  "license": "MIT",
  "author": "castLabs",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.1"
  },
  "peerDependencies": {
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.0.0",
    "rollup": "^4.46.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
